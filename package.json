{
  "name": "bmf001",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:webpack": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "generate-types": "node scripts/generate-types.js",
    "db:types": "npm run generate-types",
    "db:migrate": "node scripts/run-migrations.js",
    "db:reset": "supabase db reset",
    "db:status": "supabase db status",
    "db:seed": "tsx src/scripts/seed-cli.ts",
    "db:seed:dev": "NODE_ENV=development tsx src/scripts/seed-cli.ts",
    "db:seed:test": "NODE_ENV=test tsx src/scripts/seed-cli.ts",
    "db:seed:prod": "NODE_ENV=production tsx src/scripts/seed-cli.ts",
    "db:seed:clear": "tsx src/scripts/seed-cli.ts -- --clear",
    "workers:start": "tsx src/scripts/start-workers.ts",
    "workers:dev": "NODE_ENV=development tsx src/scripts/start-workers.ts",
    "workers:prod": "NODE_ENV=production tsx src/scripts/start-workers.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern='__tests__/unit'",
    "test:integration": "jest --testPathPattern='__tests__/integration'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:coverage": "jest --coverage",
    "test:security": "npm run test:security:owasp",
    "test:security:owasp": "docker run -t owasp/zap2docker-stable zap-baseline.py -t http://localhost:3000",
    "test:load": "k6 run tests/load/basic-load.js",
    "test:load:stress": "k6 run tests/load/stress-test.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "test:setup": "tsx tests/setup/test-setup.ts",
    "performance-comparison": "tsx src/scripts/performance-comparison.ts",
    "deploy-optimizations": "node scripts/deploy-optimizations.js",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "ANALYZE=true BUNDLE_ANALYZE=server npm run build",
    "analyze:client": "ANALYZE=true BUNDLE_ANALYZE=client npm run build",
    "bundle:size": "npm run build && npx bundlesize",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:bundle": "npm run analyze && npm run optimize:images"
  },
