{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "include": [
    "src/**/*.{ts,tsx}"
  ],
  "exclude": [
    "src/**/*.test.{ts,tsx}",
    "src/**/*.spec.{ts,tsx}",
    "src/**/*.d.ts",
    "src/types/**/*",
    "src/**/*.stories.{ts,tsx}",
    "src/app/layout.tsx",
    "src/app/loading.tsx",
    "src/app/error.tsx",
    "src/app/not-found.tsx",
    "src/middleware.ts",
    "**/__tests__/**",
    "**/node_modules/**",
    "coverage/**",
    ".next/**"
  ],
  "extension": [
    ".ts",
    ".tsx"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcov",
    "json",
    "json-summary",
    "clover"
  ],
  "report-dir": "coverage",
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80,
  "per-file": true,
  "cache": true,
  "temp-dir": "./coverage/.nyc_output"
}
